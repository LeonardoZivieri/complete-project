{
    "name": "database",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "dev": "tsc -w",
        "prisma": "dotenv -e ../../.env -- prisma",
        "db:migrate": "dotenv -e ../../.env -- prisma migrate",
        "db:generate": "dotenv -e ../../.env -- prisma generate",
        "db:push": "dotenv -e ../../.env -- prisma db push --skip-generate"
    },
    "dependencies": {
        "@prisma/client": "^4.7.1",
        "dotenv": "^16.0.3",
        "dotenv-cli": "^6.0.0"
    },
    "devDependencies": {
        "prisma": "^4.7.1"
    }
}
